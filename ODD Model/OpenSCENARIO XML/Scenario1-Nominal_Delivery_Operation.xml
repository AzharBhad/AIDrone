<?xml version="1.0" encoding="UTF-8"?>
<!--
================================================================================
ASAM OpenSCENARIO XML 1.3.1
Scenario: Nominal Delivery Operation - Urban Residential
Description: Standard package delivery under ideal conditions
Based on ODD Module: M001 - Primary_Operating_Conditions
================================================================================
-->
<OpenSCENARIO xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="OpenSCENARIO.xsd">

  <FileHeader revMajor="1"
              revMinor="3"
              date="2025-10-30T10:00:00"
              description="Nominal drone delivery scenario in urban residential area"
              author="Autonomous Systems Test Team"/>

  <!-- ========================================================================
       PARAMETER DECLARATIONS
       ======================================================================== -->
  <ParameterDeclarations>
    <ParameterDeclaration name="DroneMaxSpeed" parameterType="double" value="15.0"/>
    <ParameterDeclaration name="CruiseAltitude" parameterType="double" value="50.0"/>
    <ParameterDeclaration name="PayloadWeight" parameterType="double" value="2.5"/>
    <ParameterDeclaration name="DeliveryDistance" parameterType="double" value="3500.0"/>
    <ParameterDeclaration name="WindSpeed" parameterType="double" value="5.0"/>
    <ParameterDeclaration name="Visibility" parameterType="double" value="5000.0"/>
    <ParameterDeclaration name="Temperature" parameterType="double" value="20.0"/>
  </ParameterDeclarations>

  <!-- ========================================================================
       CATALOG LOCATIONS
       ======================================================================== -->
  <CatalogLocations>
    <VehicleCatalog>
      <Directory path="./Catalogs/Vehicles"/>
    </VehicleCatalog>
    <PedestrianCatalog>
      <Directory path="./Catalogs/Pedestrians"/>
    </PedestrianCatalog>
    <EnvironmentCatalog>
      <Directory path="./Catalogs/Environment"/>
    </EnvironmentCatalog>
    <ManeuverCatalog>
      <Directory path="./Catalogs/Maneuvers"/>
    </ManeuverCatalog>
    <TrajectoryCatalog>
      <Directory path="./Catalogs/Trajectories"/>
    </TrajectoryCatalog>
    <RouteCatalog>
      <Directory path="./Catalogs/Routes"/>
    </RouteCatalog>
  </CatalogLocations>

  <!-- ========================================================================
       ROAD NETWORK
       ======================================================================== -->
  <RoadNetwork>
    <LogicFile filepath="./RoadNetworks/UrbanResidential_01.xodr"/>
    <SceneGraphFile filepath="./RoadNetworks/UrbanResidential_01.opt.osgb"/>
    <TrafficSignals>
      <TrafficSignalController name="TrafficController_01" delay="0">
        <Phase name="Phase1" duration="30"/>
      </TrafficSignalController>
    </TrafficSignals>
  </RoadNetwork>

  <!-- ========================================================================
       ENTITIES
       ======================================================================== -->
  <Entities>
    <!-- Ego Drone -->
    <ScenarioObject name="EgoDrone">
      <CatalogReference catalogName="VehicleCatalog" entryName="DeliveryDrone_Model_X1">
        <ParameterAssignments>
          <ParameterAssignment parameterRef="MaxSpeed" value="$DroneMaxSpeed"/>
          <ParameterAssignment parameterRef="MaxPayload" value="5.0"/>
          <ParameterAssignment parameterRef="BatteryCapacity" value="100.0"/>
        </ParameterAssignments>
      </CatalogReference>
      <ObjectController>
        <CatalogReference catalogName="ControllerCatalog" entryName="AutonomousDroneController"/>
      </ObjectController>
    </ScenarioObject>

    <!-- Ambient Traffic - Car 1 -->
    <ScenarioObject name="Car_01">
      <CatalogReference catalogName="VehicleCatalog" entryName="Sedan_Blue"/>
    </ScenarioObject>

    <!-- Ambient Traffic - Car 2 -->
    <ScenarioObject name="Car_02">
      <CatalogReference catalogName="VehicleCatalog" entryName="SUV_White"/>
    </ScenarioObject>

    <!-- Pedestrian -->
    <ScenarioObject name="Pedestrian_01">
      <CatalogReference catalogName="PedestrianCatalog" entryName="Adult_Male_01"/>
    </ScenarioObject>

    <!-- Bird as environmental hazard -->
    <ScenarioObject name="Bird_Flock_01">
      <MiscObject miscObjectCategory="obstacle" mass="2.0" name="BirdFlock">
        <ParameterDeclarations/>
        <BoundingBox>
          <Center x="0.0" y="0.0" z="0.5"/>
          <Dimensions width="3.0" length="2.0" height="1.0"/>
        </BoundingBox>
        <Properties>
          <Property name="model3d" value="./models/birds_flock.osgb"/>
        </Properties>
      </MiscObject>
    </ScenarioObject>
  </Entities>

  <!-- ========================================================================
       STORYBOARD
       ======================================================================== -->
  <Storyboard>

    <!-- ======================================================================
         INIT - Initial Conditions
         ====================================================================== -->
    <Init>
      <Actions>
        <!-- Ego Drone Initial Position - At Depot -->
        <Private entityRef="EgoDrone">
          <PrivateAction>
            <TeleportAction>
              <Position>
                <WorldPosition x="1000.0" y="2000.0" z="2.0" h="0.0" p="0.0" r="0.0"/>
              </Position>
            </TeleportAction>
          </PrivateAction>
          <PrivateAction>
            <LongitudinalAction>
              <SpeedAction>
                <SpeedActionDynamics dynamicsShape="step" value="0.0" dynamicsDimension="time"/>
                <SpeedActionTarget>
                  <AbsoluteTargetSpeed value="0.0"/>
                </SpeedActionTarget>
              </SpeedAction>
            </LongitudinalAction>
          </PrivateAction>
        </Private>

        <!-- Car 1 Initial Position -->
        <Private entityRef="Car_01">
          <PrivateAction>
            <TeleportAction>
              <Position>
                <LanePosition roadId="1" laneId="-1" offset="0.0" s="50.0"/>
              </Position>
            </TeleportAction>
          </PrivateAction>
          <PrivateAction>
            <LongitudinalAction>
              <SpeedAction>
                <SpeedActionDynamics dynamicsShape="step" value="0.0" dynamicsDimension="time"/>
                <SpeedActionTarget>
                  <AbsoluteTargetSpeed value="13.89"/>
                </SpeedActionTarget>
              </SpeedAction>
            </LongitudinalAction>
          </PrivateAction>
        </Private>

        <!-- Car 2 Initial Position -->
        <Private entityRef="Car_02">
          <PrivateAction>
            <TeleportAction>
              <Position>
                <LanePosition roadId="2" laneId="-1" offset="0.0" s="100.0"/>
              </Position>
            </TeleportAction>
          </PrivateAction>
          <PrivateAction>
            <LongitudinalAction>
              <SpeedAction>
                <SpeedActionDynamics dynamicsShape="step" value="0.0" dynamicsDimension="time"/>
                <SpeedActionTarget>
                  <AbsoluteTargetSpeed value="11.11"/>
                </SpeedActionTarget>
              </SpeedAction>
            </LongitudinalAction>
          </PrivateAction>
        </Private>

        <!-- Pedestrian Initial Position -->
        <Private entityRef="Pedestrian_01">
          <PrivateAction>
            <TeleportAction>
              <Position>
                <WorldPosition x="2500.0" y="2100.0" z="0.0" h="1.57" p="0.0" r="0.0"/>
              </Position>
            </TeleportAction>
          </PrivateAction>
          <PrivateAction>
            <LongitudinalAction>
              <SpeedAction>
                <SpeedActionDynamics dynamicsShape="step" value="0.0" dynamicsDimension="time"/>
                <SpeedActionTarget>
                  <AbsoluteTargetSpeed value="1.4"/>
                </SpeedActionTarget>
              </SpeedAction>
            </LongitudinalAction>
          </PrivateAction>
        </Private>

        <!-- Bird Flock Initial Position -->
        <Private entityRef="Bird_Flock_01">
          <PrivateAction>
            <TeleportAction>
              <Position>
                <WorldPosition x="3000.0" y="2300.0" z="45.0" h="0.0" p="0.0" r="0.0"/>
              </Position>
            </TeleportAction>
          </PrivateAction>
          <PrivateAction>
            <VisibilityAction graphics="false" traffic="true" sensors="true"/>
          </PrivateAction>
        </Private>
      </Actions>
    </Init>

    <!-- ======================================================================
         STORY - Mission Execution
         ====================================================================== -->
    <Story name="DeliveryMission">

      <!-- ====================================================================
           ACT 1: Takeoff and Initial Ascent
           ==================================================================== -->
      <Act name="TakeoffSequence">
        <ManeuverGroup maximumExecutionCount="1" name="DroneTakeoff">
          <Actors selectTriggeringEntities="false">
            <EntityRef entityRef="EgoDrone"/>
          </Actors>

          <Maneuver name="VerticalTakeoff">
            <Event name="StartTakeoff" priority="overwrite">
              <Action name="TakeoffAction">
                <PrivateAction>
                  <LongitudinalAction>
                    <SpeedAction>
                      <SpeedActionDynamics dynamicsShape="linear" value="3.0" dynamicsDimension="time"/>
                      <SpeedActionTarget>
                        <AbsoluteTargetSpeed value="5.0"/>
                      </SpeedActionTarget>
                    </SpeedAction>
                  </LongitudinalAction>
                </PrivateAction>
              </Action>
              <Action name="ClimbAction">
                <PrivateAction>
                  <RoutingAction>
                    <AcquirePositionAction>
                      <Position>
                        <RelativeWorldPosition entityRef="EgoDrone" dx="0.0" dy="0.0" dz="$CruiseAltitude"/>
                      </Position>
                    </AcquirePositionAction>
                  </RoutingAction>
                </PrivateAction>
              </Action>
              <StartTrigger>
                <ConditionGroup>
                  <Condition name="StartCondition" delay="0" conditionEdge="rising">
                    <ByValueCondition>
                      <SimulationTimeCondition value="2.0" rule="greaterThan"/>
                    </ByValueCondition>
                  </Condition>
                </ConditionGroup>
              </StartTrigger>
            </Event>
          </Maneuver>
        </ManeuverGroup>

        <StartTrigger>
          <ConditionGroup>
            <Condition name="ActStartCondition" delay="0" conditionEdge="rising">
              <ByValueCondition>
                <SimulationTimeCondition value="0.0" rule="greaterThan"/>
              </ByValueCondition>
            </Condition>
          </ConditionGroup>
        </StartTrigger>

        <StopTrigger>
          <ConditionGroup>
            <Condition name="TakeoffComplete" delay="0" conditionEdge="rising">
              <ByEntityCondition>
                <TriggeringEntities triggeringEntitiesRule="any">
                  <EntityRef entityRef="EgoDrone"/>
                </TriggeringEntities>
                <EntityCondition>
                  <ReachPositionCondition tolerance="2.0">
                    <Position>
                      <RelativeWorldPosition entityRef="EgoDrone" dx="0.0" dy="0.0" dz="$CruiseAltitude"/>
                    </Position>
                  </ReachPositionCondition>
                </EntityCondition>
              </ByEntityCondition>
            </Condition>
          </ConditionGroup>
        </StopTrigger>
      </Act>

      <!-- ====================================================================
           ACT 2: Cruise Flight to Delivery Location
           ==================================================================== -->
      <Act name="CruiseFlight">
        <ManeuverGroup maximumExecutionCount="1" name="NavigateToDestination">
          <Actors selectTriggeringEntities="false">
            <EntityRef entityRef="EgoDrone"/>
          </Actors>

          <Maneuver name="FollowFlightPath">
            <Event name="NavigateRoute" priority="overwrite">
              <Action name="FollowTrajectory">
                <PrivateAction>
                  <RoutingAction>
                    <FollowTrajectoryAction>
                      <Trajectory name="DeliveryRoute" closed="false">
                        <ParameterDeclarations/>
                        <Shape>
                          <Polyline>
                            <Vertex time="0.0">
                              <Position>
                                <WorldPosition x="1000.0" y="2000.0" z="$CruiseAltitude"/>
                              </Position>
                            </Vertex>
                            <Vertex time="50.0">
                              <Position>
                                <WorldPosition x="1500.0" y="2200.0" z="$CruiseAltitude"/>
                              </Position>
                            </Vertex>
                            <Vertex time="100.0">
                              <Position>
                                <WorldPosition x="2000.0" y="2400.0" z="$CruiseAltitude"/>
                              </Position>
                            </Vertex>
                            <Vertex time="150.0">
                              <Position>
                                <WorldPosition x="2500.0" y="2500.0" z="$CruiseAltitude"/>
                              </Position>
                            </Vertex>
                            <Vertex time="200.0">
                              <Position>
                                <WorldPosition x="3000.0" y="2600.0" z="$CruiseAltitude"/>
                              </Position>
                            </Vertex>
                            <Vertex time="233.0">
                              <Position>
                                <WorldPosition x="3500.0" y="2700.0" z="$CruiseAltitude"/>
                              </Position>
                            </Vertex>
                          </Polyline>
                        </Shape>
                      </Trajectory>
                      <TimeReference>
                        <Timing domainAbsoluteRelative="relative" scale="1.0" offset="0.0"/>
                      </TimeReference>
                      <TrajectoryFollowingMode followingMode="position"/>
                    </FollowTrajectoryAction>
                  </RoutingAction>
                </PrivateAction>
              </Action>
              <StartTrigger>
                <ConditionGroup>
                  <Condition name="StartNavigation" delay="0" conditionEdge="rising">
                    <ByValueCondition>
                      <SimulationTimeCondition value="15.0" rule="greaterThan"/>
                    </ByValueCondition>
                  </Condition>
                </ConditionGroup>
              </StartTrigger>
            </Event>

            <!-- Bird Encounter Event -->
            <Event name="BirdEncounter" priority="overwrite">
              <Action name="ActivateBirdFlock">
                <PrivateAction>
                  <VisibilityAction graphics="true" traffic="true" sensors="true"/>
                </PrivateAction>
              </Action>
              <Action name="BirdMovement">
                <PrivateAction>
                  <LongitudinalAction>
                    <SpeedAction>
                      <SpeedActionDynamics dynamicsShape="linear" value="2.0" dynamicsDimension="time"/>
                      <SpeedActionTarget>
                        <AbsoluteTargetSpeed value="8.0"/>
                      </SpeedActionTarget>
                    </SpeedAction>
                  </LongitudinalAction>
                </PrivateAction>
              </Action>
              <StartTrigger>
                <ConditionGroup>
                  <Condition name="BirdEncounterTrigger" delay="0" conditionEdge="rising">
                    <ByValueCondition>
                      <SimulationTimeCondition value="120.0" rule="greaterThan"/>
                    </ByValueCondition>
                  </Condition>
                </ConditionGroup>
              </StartTrigger>
            </Event>
          </Maneuver>
        </ManeuverGroup>

        <StartTrigger>
          <ConditionGroup>
            <Condition name="CruiseStart" delay="0" conditionEdge="rising">
              <ByValueCondition>
                <StoryboardElementStateCondition storyboardElementType="act"
                                                 storyboardElementRef="TakeoffSequence"
                                                 state="completeState"/>
              </ByValueCondition>
            </Condition>
          </ConditionGroup>
        </StartTrigger>

        <StopTrigger>
          <ConditionGroup>
            <Condition name="ApproachingDestination" delay="0" conditionEdge="rising">
              <ByEntityCondition>
                <TriggeringEntities triggeringEntitiesRule="any">
                  <EntityRef entityRef="EgoDrone"/>
                </TriggeringEntities>
                <EntityCondition>
                  <ReachPositionCondition tolerance="10.0">
                    <Position>
                      <WorldPosition x="3500.0" y="2700.0" z="$CruiseAltitude"/>
                    </Position>
                  </ReachPositionCondition>
                </EntityCondition>
              </ByEntityCondition>
            </Condition>
          </ConditionGroup>
        </StopTrigger>
      </Act>

      <!-- ====================================================================
           ACT 3: Descent and Package Delivery
           ==================================================================== -->
      <Act name="DeliverySequence">
        <ManeuverGroup maximumExecutionCount="1" name="LandingAndDelivery">
          <Actors selectTriggeringEntities="false">
            <EntityRef entityRef="EgoDrone"/>
          </Actors>

          <Maneuver name="PrecisionLanding">
            <Event name="DescentToLandingZone" priority="overwrite">
              <Action name="ReduceSpeed">
                <PrivateAction>
                  <LongitudinalAction>
                    <SpeedAction>
                      <SpeedActionDynamics dynamicsShape="linear" value="5.0" dynamicsDimension="time"/>
                      <SpeedActionTarget>
                        <AbsoluteTargetSpeed value="2.0"/>
                      </SpeedActionTarget>
                    </SpeedAction>
                  </LongitudinalAction>
                </PrivateAction>
              </Action>
              <Action name="DescendToGround">
                <PrivateAction>
                  <RoutingAction>
                    <AcquirePositionAction>
                      <Position>
                        <WorldPosition x="3500.0" y="2700.0" z="0.5"/>
                      </Position>
                    </AcquirePositionAction>
                  </RoutingAction>
                </PrivateAction>
              </Action>
              <StartTrigger>
                <ConditionGroup>
                  <Condition name="StartDescent" delay="0" conditionEdge="rising">
                    <ByValueCondition>
                      <SimulationTimeCondition value="250.0" rule="greaterThan"/>
                    </ByValueCondition>
                  </Condition>
                </ConditionGroup>
              </StartTrigger>
            </Event>

            <Event name="PackageRelease" priority="overwrite">
              <Action name="ReleasePackage">
                <UserDefinedAction>
                  <CustomCommandAction type="DeliverPackage">
                    <Property name="packageId" value="PKG-2025-001234"/>
                    <Property name="deliveryConfirmation" value="true"/>
                  </CustomCommandAction>
                </UserDefinedAction>
              </Action>
              <StartTrigger>
                <ConditionGroup>
                  <Condition name="TouchdownComplete" delay="2.0" conditionEdge="rising">
                    <ByEntityCondition>
                      <TriggeringEntities triggeringEntitiesRule="any">
                        <EntityRef entityRef="EgoDrone"/>
                      </TriggeringEntities>
                      <EntityCondition>
                        <ReachPositionCondition tolerance="0.5">
                          <Position>
                            <WorldPosition x="3500.0" y="2700.0" z="0.5"/>
                          </Position>
                        </ReachPositionCondition>
                      </EntityCondition>
                    </ByEntityCondition>
                  </Condition>
                </ConditionGroup>
              </StartTrigger>
            </Event>
          </Maneuver>
        </ManeuverGroup>

        <StartTrigger>
          <ConditionGroup>
            <Condition name="DeliveryStart" delay="0" conditionEdge="rising">
              <ByValueCondition>
                <StoryboardElementStateCondition storyboardElementType="act"
                                                 storyboardElementRef="CruiseFlight"
                                                 state="completeState"/>
              </ByValueCondition>
            </Condition>
          </ConditionGroup>
        </StartTrigger>

        <StopTrigger>
          <ConditionGroup>
            <Condition name="DeliveryComplete" delay="5.0" conditionEdge="rising">
              <ByValueCondition>
                <StoryboardElementStateCondition storyboardElementType="event"
                                                 storyboardElementRef="PackageRelease"
                                                 state="completeState"/>
              </ByValueCondition>
            </Condition>
          </ConditionGroup>
        </StopTrigger>
      </Act>

      <!-- ====================================================================
           ACT 4: Return to Base
           ==================================================================== -->
      <Act name="ReturnToBase">
        <ManeuverGroup maximumExecutionCount="1" name="RTBSequence">
          <Actors selectTriggeringEntities="false">
            <EntityRef entityRef="EgoDrone"/>
          </Actors>

          <Maneuver name="ClimbAndReturn">
            <Event name="TakeoffFromDelivery" priority="overwrite">
              <Action name="ClimbToCruise">
                <PrivateAction>
                  <RoutingAction>
                    <AcquirePositionAction>
                      <Position>
                        <WorldPosition x="3500.0" y="2700.0" z="$CruiseAltitude"/>
                      </Position>
                    </AcquirePositionAction>
                  </RoutingAction>
                </PrivateAction>
              </Action>
              <StartTrigger>
                <ConditionGroup>
                  <Condition name="StartRTB" delay="0" conditionEdge="rising">
                    <ByValueCondition>
                      <SimulationTimeCondition value="280.0" rule="greaterThan"/>
                    </ByValueCondition>
                  </Condition>
                </ConditionGroup>
              </StartTrigger>
            </Event>

            <Event name="ReturnFlight" priority="overwrite">
              <Action name="NavigateToDepot">
                <PrivateAction>
                  <RoutingAction>
                    <FollowTrajectoryAction>
                      <Trajectory name="ReturnRoute" closed="false">
                        <ParameterDeclarations/>
                        <Shape>
                          <Polyline>
                            <Vertex time="0.0">
                              <Position>
                                <WorldPosition x="3500.0" y="2700.0" z="$CruiseAltitude"/>
                              </Position>
                            </Vertex>
                            <Vertex time="60.0">
                              <Position>
                                <WorldPosition x="3000.0" y="2500.0" z="$CruiseAltitude"/>
                              </Position>
                            </Vertex>
                            <Vertex time="120.0">
                              <Position>
                                <WorldPosition x="2500.0" y="2300.0" z="$CruiseAltitude"/>
                              </Position>
                            </Vertex>
                            <Vertex time="180.0">
                              <Position>
                                <WorldPosition x="2000.0" y="2200.0" z="$CruiseAltitude"/>
                              </Position>
                            </Vertex>
                            <Vertex time="233.0">
                              <Position>
                                <WorldPosition x="1000.0" y="2000.0" z="$CruiseAltitude"/>
                              </Position>
                            </Vertex>
                          </Polyline>
                        </Shape>
                      </Trajectory>
                      <TimeReference>
                        <Timing domainAbsoluteRelative="relative" scale="1.0" offset="0.0"/>
                      </TimeReference>
                      <TrajectoryFollowingMode followingMode="position"/>
                    </FollowTrajectoryAction>
                  </RoutingAction>
                </PrivateAction>
              </Action>
              <StartTrigger>
                <ConditionGroup>
                  <Condition name="ClimbComplete" delay="0" conditionEdge="rising">
                    <ByEntityCondition>
                      <TriggeringEntities triggeringEntitiesRule="any">
                        <EntityRef entityRef="EgoDrone"/>
                      </TriggeringEntities>
                      <EntityCondition>
                        <ReachPositionCondition tolerance="2.0">
                          <Position>
                            <WorldPosition x="3500.0" y="2700.0" z="$CruiseAltitude"/>
                          </Position>
                        </ReachPositionCondition>
                      </EntityCondition>
                    </ByEntityCondition>
                  </Condition>
                </ConditionGroup>
              </StartTrigger>
            </Event>

            <Event name="FinalLanding" priority="overwrite">
              <Action name="LandAtDepot">
                <PrivateAction>
                  <RoutingAction>
                    <AcquirePositionAction>
                      <Position>
                        <WorldPosition x="1000.0" y="2000.0" z="0.5"/>
                      </Position>
                    </AcquirePositionAction>
                  </RoutingAction>
                </PrivateAction>
              </Action>
              <StartTrigger>
                <ConditionGroup>
                  <Condition name="ApproachingDepot" delay="0" conditionEdge="rising">
                    <ByEntityCondition>
                      <TriggeringEntities triggeringEntitiesRule="any">
                        <EntityRef entityRef="EgoDrone"/>
                      </TriggeringEntities>
                      <EntityCondition>
                        <ReachPositionCondition tolerance="10.0">
                          <Position>
                            <WorldPosition x="1000.0" y="2000.0" z="$CruiseAltitude"/>
                          </Position>
                        </ReachPositionCondition>
                      </EntityCondition>
                    </ByEntityCondition>
                  </Condition>
                </ConditionGroup>
              </StartTrigger>
            </Event>
          </Maneuver>
        </ManeuverGroup>

        <StartTrigger>
          <ConditionGroup>
            <Condition name="RTBStart" delay="0" conditionEdge="rising">
              <ByValueCondition>
                <StoryboardElementStateCondition storyboardElementType="act"
                                                 storyboardElementRef="DeliverySequence"
                                                 state="completeState"/>
              </ByValueCondition>
            </Condition>
          </ConditionGroup>
        </StartTrigger>
      </Act>
    </Story>

    <!-- ======================================================================
         STOP TRIGGER - Simulation End Conditions
         ====================================================================== -->
    <StopTrigger>
      <ConditionGroup>
        <Condition name="MissionComplete" delay="0" conditionEdge="rising">
          <ByValueCondition>
            <StoryboardElementStateCondition storyboardElementType="act"
                                             storyboardElementRef="ReturnToBase"
                                             state="completeState"/>
          </ByValueCondition>
        </Condition>
      </ConditionGroup>
      <ConditionGroup>
        <Condition name="MaxSimulationTime" delay="0" conditionEdge="rising">
          <ByValueCondition>
            <SimulationTimeCondition value="600.0" rule="greaterThan"/>
          </ByValueCondition>
        </Condition>
      </ConditionGroup>
    </StopTrigger>
  </Storyboard>

</OpenSCENARIO>